    "drupal/clean_maintenance": "^1.6"
  },
  "config": {
    "vendor-dir": "vendor"
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "find \"sites/all/\" -name .git -type d -exec rm -rf {} +",
      "rm -rf \"core\""
    ],
    "post-update-cmd": [
      "find \"sites/all/\" -name .git -type d -exec rm -rf {} +",
      "rm -rf \"core\""
    ]
  },
  "extra": {
    "patches": {
      "drupal/panels": {
        "Issue 1402860 : Panelizer is incompatible with moderation": "https://www.drupal.org/files/issues/panelizer_is-1402860-82-fix-ipe-end-js-alert.patch",
        "Issue 2485837 : Refresh the Workbench block after editing with Panels IPE": "https://www.drupal.org/files/issues/panels-ipe-workbench-block-2485837-1.patch"
      },
      "drupal/panelizer": {
        "Issue 2457113 : Better Revision Handling inside and outside of Workbench Moderation": "https://www.drupal.org/files/issues/panelizer-2457113-workbench-moderation-72.patch"
      },
      "drupal/redirect": {
        "Issue 905914 : Merge global redirect functions into Redirect module": "https://www.drupal.org/files/issues/redirect-merge_global_redirect-905914-246-d7.patch"
      }
    },
    "installer-types": ["library"],
    "installer-paths": {
      "sites/all/libraries/": ["factorialio/bi-blueprint-lib"],
      "sites/all/modules/custom/{$name}": ["type:drupal-custom-module"],
      "sites/all/modules/contrib/{$name}": ["type:drupal-module"],
      "sites/all/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "sites/all/themes/contrib/{$name}": ["type:drupal-theme"],
      "sites/all/themes/engines/tfd7": ["drupal/tfd7"],
      "sites/all/profiles/contrib/{$name}": ["type:drupal-profile"],
      ".tools/": ["factorial-io/fabalicious"]
    },
    "preserve-paths": [
      "sites/all/modules/contrib",
      "sites/all/modules/custom",
      "sites/all/modules/features",
      "sites/all/themes/contrib",
      "sites/all/themes/custom",
      "sites/all/libraries",
      "sites/all/drush",
      "sites/default/settings.php",
      "sites/default/files"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {}
}
