/**
 * Implements hook_potx_file_location_info().
 *
 * Hook used to define the location where to find the po files.
 * A po file has a name of the form "name.ln.po" where ln
 * is the language code.
 *
 * @return
 *   An associative array with a key and a path.
 */
function es_helper_potx_file_location_info() {
  $paths = [];
{% if po_dir is not empty %}
  $paths = [
{% for path in po_dir %}
    'es_helper_translations_{{ loop.index }}' => [
      'path' => '{{ path }}',
    ],
{% endfor %}
  ];
{% endif %}
  return $paths;
}
