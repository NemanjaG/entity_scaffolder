name = FE ES
core = 7.x
package = Features
version = 7.x-1.0
dependencies[] = ckeditor
dependencies[] = ctools
dependencies[] = features
dependencies[] = filter
{% for dependency in dependencies %}
dependencies[] = dependency
{% endfor %}
features[ckeditor_profile][] = HTML
features[ctools][] = fieldable_panels_panes:fieldable_panels_pane_type:1
features[filter][] = html
features[filter][] = plain_text
project path = sites/all/modules/features
features[ctools][] = strongarm:strongarm:1
features[features_api][] = api:2
{% for feature in features.field_base %}
features[field_base][] = feature
{% endfor %}
{% for feature in features.field_instance %}
features[field_base][] = feature
{% endfor %}
{% for feature in features.fieldable_panels_pane_type %}
features[fieldable_panels_pane_type][] = feature
{% endfor %}
